{
  "name": "proxy-subgraph",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "tsc && yarn run build:gql",
    "build:gql": "graphql-codegen --config ./gql/codegen.yml",
    "start": "yarn build && node dist/src/server.js",
    "prod": "yarn build && pm2 start dist/src/server.js -i 3 --cron-restart='0 */6 * * *' --wait-ready --listen-timeout=5000 --max-memory-restart 200M",
    "pm2-stop": "pm2 stop dist/src/server.js",
    "pm2-list": "pm2 list",
    "pm2-logs": "pm2 log",
    "pm2-clean-logs": "pm2 flush",
    "dev": "tsc && node dist/src/server.js"
  },
  "dependencies": {
    "@apollo/client": "^3.8.8",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "graphql": "^16.8.1",
    "node-schedule": "^2.1.1",
    "pm2": "^5.4.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.12",
    "@types/node-schedule": "^2.1.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
